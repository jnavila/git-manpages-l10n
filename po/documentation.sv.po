msgstr ""

#. type: Labeled list
#: en/diff-options.adoc:58
#, no-wrap
#, ignore-same, no-wrap, priority:280
msgid "`--remerge-diff`"
msgstr "`--remerge-diff`"

#. type: Plain text
#: en/diff-options.adoc:61
msgid "Produce remerge-diff output for merge commits. Shortcut for `--diff-merges=remerge -p`."
msgstr ""

#. type: Labeled list
#: en/diff-options.adoc:62
#, no-wrap
#, ignore-same, no-wrap, priority:280
