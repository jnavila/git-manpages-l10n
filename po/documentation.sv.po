msgstr ""

#. type: Labeled list
#: en/for-each-ref-options.adoc:52
#, no-wrap
msgid "`--no-merged[=<object>]`"
msgstr ""

#. type: Plain text
#: en/for-each-ref-options.adoc:55
msgid "Only list refs whose tips are not reachable from _<object>_(`HEAD` if not specified)."
msgstr ""

#. type: Labeled list
#: en/for-each-ref-options.adoc:56
#, no-wrap
msgid "`--contains[=<object>]`"
msgstr ""

#. type: Plain text
