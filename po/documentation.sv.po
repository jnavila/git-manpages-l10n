#. type: Labeled list
#: en/diff-options.adoc:45 en/git-branch.adoc:138
#, no-wrap
#, ignore-same, no-wrap, priority:280
msgid "`-c`"
msgstr "`-c`"

#. type: Plain text
#: en/diff-options.adoc:48
msgid "Produce combined diff output for merge commits. Shortcut for `--diff-merges=combined -p`."
msgstr ""

#. type: Labeled list
#: en/diff-options.adoc:49
#, no-wrap
#, ignore-same, no-wrap, priority:280
